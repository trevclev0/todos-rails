<h1>Login</h1>
<%= form_with url: login_path, method: :post, local: true do |form| %>
  <div>
    <%= form.label :username %>
    <%= form.text_field :username, autocomplete: 'username', required: true %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password, autocomplete: 'current-password', required: true %>
  </div>
  <div>
    <%= form.submit 'Login' %>
  </div>
<% end %>
